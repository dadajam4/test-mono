{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "prebuild": {
      "outputs": ["dist/**"]
    },
    "predev": {
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "lint": {},
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    }
  }
}
